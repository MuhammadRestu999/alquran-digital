    },
    {
      "ayah": 5,
      "quarter_hizb": 60.75,
      "juz": 30,
      "manzil": 7,
      "arabic": "وَمِنْ شَرِّ حَاسِدٍ اِذَا حَسَدَ ࣖ",
      "latin": "Wa min syarri ḥāsidin iżā ḥasad(a).",
